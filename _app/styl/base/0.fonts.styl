G = 'Gilroy', sans-serif
GBk = 'GilroyBlack', sans-serif
GBd = 'GilroyBold', sans-serif
GM = 'GilroyMedium', sans-serif
GL = 'GilroyLight', sans-serif

@font-face {
	font-family: 'Gilroy';
	font-display: fallback;
	font-weight: normal;
	font-style: normal;
	src: local  url('../fonts/Gilroy.eot');
	src: url('../fonts/Gilroy.eot');
	src: url('../fonts/Gilroy.eot') format('embedded-opentype'),
		 url('../fonts/Gilroy.woff2') format('woff2'),
		 url('../fonts/Gilroy.woff') format('woff'),
		 url('../fonts/Gilroy.ttf') format('truetype'),
		 url('../fonts/Gilroy.svg#Gilroy') format('svg');
}
